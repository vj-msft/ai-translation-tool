# Azure AI Foundry Configuration
# Copy this file to .env and replace these with your actual Azure AI Foundry values

# Your Azure AI Foundry endpoint (replace with your actual endpoint)
VITE_AZURE_OPENAI_ENDPOINT=https://your-resource.cognitiveservices.azure.com

# Your Azure AI Foundry API key (replace with your actual API key)
VITE_AZURE_OPENAI_API_KEY=your-api-key-here

# Model deployment names in your Azure AI Foundry resource (replace with your actual deployment names)
VITE_GPT4_DEPLOYMENT_NAME=gpt-4o
VITE_GPT5_DEPLOYMENT_NAME=gpt-5
VITE_GPT41_DEPLOYMENT_NAME=gpt-4.1

# API Version
VITE_AZURE_OPENAI_API_VERSION=2024-02-15-preview
